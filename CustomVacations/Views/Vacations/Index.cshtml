@model IEnumerable<VacationModel>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div style="height: 10vw"></div>

<h2>Index</h2>

<!--User fills out vacation request form. If they don't and hit <submit> it returns the list of suggested vacations.-->
<form>
    <div class="form-row">
        <div class="form-group col-md-5">
            <label id="firstname">First Name: </label>
            <input type="text" name="fname" id="firstname" /><br />

            <label id="lastname">Last Name</label>
            <input type="text" name="lname" id="lastname" /><br />


        </div>
    </div>
</form>
